<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="MadPurgeHistoryDialog">
		<title>Purge Histories</title>
		<centered>1</centered>
		<id_arg>0</id_arg>
		<style>wxCAPTION|wxSYSTEM_MENU|wxCLOSE_BOX</style>
		<handler function="MadPurgeHistoryDialogClose" entry="EVT_CLOSE" />
		<handler function="MadPurgeHistoryDialogKeyDown" entry="EVT_KEY_DOWN" />
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxRecentFiles" member="yes">
							<label>Recent &amp;Files</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxRecentFonts" member="yes">
							<label>Recent Fon&amp;ts</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxRecentEncodings" member="yes">
							<label>Recent &amp;Encodings</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxRecentSearchedTexts" member="yes">
							<label>Recent &amp;Searched Texts</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxRecentReplacedTexts" member="yes">
							<label>Recent &amp;Replaced Texts</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxRecentSearchedDirectories" member="yes">
							<label>Recent Searched &amp;Directories</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxRecentSearchedFileFilters" member="yes">
							<label>Recent Searched File Fi&amp;lters</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxRecentSearchedExcludeFilters" member="yes">
							<label>Recent Searched E&amp;xclude Filters</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxCaretPos" member="yes">
							<label>Recent Caret Position</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxResetTransparency" member="yes">
							<label>Reset Transparency of Search/Replace Dialog</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxResetToolBarsInfoWin" member="yes">
							<label>Reset Tool Bars and Information Window Pos</label>
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="wxID_ANY" variable="wxCheckBoxAllAbove" member="yes">
							<label>All Above</label>
							<handler function="wxCheckBoxAllAboveClick" entry="EVT_CHECKBOX" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
				</object>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer2" member="no">
					<object class="sizeritem">
						<object class="wxButton" name="wxID_OK" variable="WxButtonOK" member="yes">
							<label>&amp;OK</label>
							<handler function="WxButtonOKClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="wxID_CANCEL" variable="WxButtonCancel" member="yes">
							<label>&amp;Cancel</label>
							<default>1</default>
							<handler function="WxButtonCancelClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</wxsmith>
